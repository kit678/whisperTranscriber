{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./",
        "lib": [
            "ESNext"
        ],
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": false,
        "allowImportingTsExtensions": false
    },
    "include": [
        "./**/*"
    ]
}